{
  "id": "dummyjson",
  "strategy": "http_api",
  "auth": {
    "login": {
      "url": "https://dummyjson.com/auth/login",
      "method": "POST",
      "payload": { "username": "{{username}}", "password": "{{password}}", "expiresInMins": 30 },
